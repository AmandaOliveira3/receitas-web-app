<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Receitas</title>
</head>
<body>
    <h1>Lista de Receitas</h1>
    <ul>
        <% receitas.forEach(function(receita) { %>
            <li><a href="/receitas/<%= receita.nome %>"><%= receita.nome %></a></li>
        <% }); %>
    </ul>
</body>
</html>
